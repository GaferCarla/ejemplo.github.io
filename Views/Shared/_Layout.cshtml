<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/indexScript")
    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/Scripts/jquery-3.1.1.min.js")
    


    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
</head>
<body>
    <header>
        <div class="logo" href="@Url.Action("Home")" style="cursor:pointer">
            <img src="~/image/logo.png" />
        </div>
        <div class="greetingUsser">
            <ul class="menuLogin">
                <li>
                    <span class="greeting" style="font-weight: normal;">Bienvenido(a) &nbsp;</span>
                </li>
                <li>
                    <a>
                        <span class="usser">Usuario</span>
                        <span class="iconDownArrow">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="12" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z" />
                            </svg>
                            &nbsp;&nbsp;&nbsp;&nbsp;
                        </span>
                        <span class="iconLoginUsser">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-fill" viewBox="0 0 16 16">
                                <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                            </svg>

                        </span>

                    </a>
                    <ul class="submenuLogin">
                        <li><a href="#">Perfil</a></li>
                        <li><a href="#">Cerrar sesión</a></li>
                    </ul>

                </li>
            </ul>


        </div>

    </header>

    <div id="sidebar">

        <div class="toggle-btn" onclick="toggleSideBar()">
            <div><span></span> <span></span> <span></span></div>
        </div>
        <div class="sideContener">
            <div class="navbarencabezados">
                <span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-gear-fill" viewBox="0 0 16 16">
                        <path d="M9.405 1.05c-.413-1.4-2.397-1.4-2.81 0l-.1.34a1.464 1.464 0 0 1-2.105.872l-.31-.17c-1.283-.698-2.686.705-1.987 1.987l.169.311c.446.82.023 1.841-.872 2.105l-.34.1c-1.4.413-1.4 2.397 0 2.81l.34.1a1.464 1.464 0 0 1 .872 2.105l-.17.31c-.698 1.283.705 2.686 1.987 1.987l.311-.169a1.464 1.464 0 0 1 2.105.872l.1.34c.413 1.4 2.397 1.4 2.81 0l.1-.34a1.464 1.464 0 0 1 2.105-.872l.31.17c1.283.698 2.686-.705 1.987-1.987l-.169-.311a1.464 1.464 0 0 1 .872-2.105l.34-.1c1.4-.413 1.4-2.397 0-2.81l-.34-.1a1.464 1.464 0 0 1-.872-2.105l.17-.31c.698-1.283-.705-2.686-1.987-1.987l-.311.169a1.464 1.464 0 0 1-2.105-.872l-.1-.34zM8 10.93a2.929 2.929 0 1 1 0-5.86 2.929 2.929 0 0 1 0 5.858z" />
                    </svg>Configuración
                </span>
            </div>
            <ul>
                <li class='sub-menu'>
                    <a href='#settings'>
                        Gestionar usuarios <svg xmlns="http://www.w3.org/2000/svg" width="16" height="12" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z" />
                        </svg>
                    </a>
                    <ul class="tabs">
                        <li><a class="subMenuList" href='@Url.Action("Añadir_usuarios","Home")'>Añadir usuarios</a></li>
                        <li><a class="subMenuList" href='@Url.Action("Gestionar_permisos","Home")'>Gestionar permisos</a></li>
                    </ul>
                </li>
                <li class='sub-menu'>
                    <a href='#settings'>
                        Encuestas <svg xmlns="http://www.w3.org/2000/svg" width="16" height="12" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z" />
                        </svg>
                    </a>
                    <ul>
                        <li><a class="subMenuList" href='@Url.Action("Añadir_encuesta","Home")'>Añadir encuesta</a></li>
                        <li><a class="subMenuList" href='@Url.Action("Editar_encuesta","Home")'>Editar encuesta</a></li>
                    </ul>
                </li>
            </ul>

            <hr />

            <div class="navbarencabezados">
                <span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-people-fill" viewBox="0 0 16 16">
                        <path d="M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                        <path fill-rule="evenodd" d="M5.216 14A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216z" />
                        <path d="M4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z" />
                    </svg>
                    Mis usuarios
                </span>
            </div>

            <ul>

                <li><a class="subMenuList" href='@Url.Action("Lista_de_usuarios","Home")'>Lista de usuarios</a></li>
                <li><a class="subMenuList" href='@Url.Action("Resultados_de_mis_usuarios","Home")'>Resultados de mis usuarios</a></li>
            </ul>

            <hr />

            <div class="navbarencabezados">
                <span>
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clipboard" viewBox="0 0 16 16">
                        <path d="M4 1.5H3a2 2 0 0 0-2 2V14a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V3.5a2 2 0 0 0-2-2h-1v1h1a1 1 0 0 1 1 1V14a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V3.5a1 1 0 0 1 1-1h1v-1z" />
                        <path d="M9.5 1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5h3zm-3-1A1.5 1.5 0 0 0 5 1.5v1A1.5 1.5 0 0 0 6.5 4h3A1.5 1.5 0 0 0 11 2.5v-1A1.5 1.5 0 0 0 9.5 0h-3z" />
                    </svg>
                    Mis encuestas
                </span>
            </div>

            <ul>

                <li><a class="subMenuList" href='@Url.Action("Encuestas_asignadas","Home")'>Encuestas asignadas</a></li>
                <li><a class="subMenuList" href='@Url.Action("Ver_mis_resultados","Home")'>Ver mis resultados</a></li>
            </ul>
        </div>




    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>

    <div class="footer">
        &#169; 2022 | CastSoft

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

    @RenderSection("scripts", required: false)
    <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js'></script>

    @Scripts.Render("~/bundles/indexScript")


</body>
</html>
